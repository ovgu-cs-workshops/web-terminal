    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "materialize-css": "^1.0.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.2",
    "preact": "^8.3.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.1.0",
    "ts-loader": "^5.3.0",
    "tslint": "^5.11.0",
    "tslint-config-standard-plus": "^2.1.1",
    "typescript": "^3.1.6",
    "unistore": "^3.1.0",
    "webpack": "^4.25.1",
