{
  "name": "web-terminal",
  "version": "0.0.0",
  "private": true,
  "description": "WAMP based web terminal backed by docker",
  "author": "Martin Koppehel <martin@embedded.enterprises>",
  "license": "MIT",
  "main": "public/index.html",
  "scripts": {
    "clean": "rm -rf dist .cache",
